{
  "installCommand": "npm install --legacy --legacy-peer-deps",
  "version": 2,
  "build": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/", "method": ["GET", "POST", "PUT", "DELETE"] }]
}
